#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/miguel/Escritorio/vscode/Mi web/visonixro-enhanced/node_modules/jiti/bin/node_modules:/home/miguel/Escritorio/vscode/Mi web/visonixro-enhanced/node_modules/jiti/node_modules:/home/miguel/Escritorio/vscode/Mi web/visonixro-enhanced/node_modules:/home/miguel/Escritorio/vscode/Mi web/node_modules:/home/miguel/Escritorio/vscode/node_modules:/home/miguel/Escritorio/node_modules:/home/miguel/node_modules:/home/node_modules:/node_modules:/home/miguel/Escritorio/vscode/Mi web/visonixro-enhanced/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/miguel/Escritorio/vscode/Mi web/visonixro-enhanced/node_modules/jiti/bin/node_modules:/home/miguel/Escritorio/vscode/Mi web/visonixro-enhanced/node_modules/jiti/node_modules:/home/miguel/Escritorio/vscode/Mi web/visonixro-enhanced/node_modules:/home/miguel/Escritorio/vscode/Mi web/node_modules:/home/miguel/Escritorio/vscode/node_modules:/home/miguel/Escritorio/node_modules:/home/miguel/node_modules:/home/node_modules:/node_modules:/home/miguel/Escritorio/vscode/Mi web/visonixro-enhanced/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jiti/bin/jiti.js" "$@"
else
  exec node  "$basedir/../jiti/bin/jiti.js" "$@"
fi
